each item in items
    div.row 
        div.six.columns
            a(href="#", onClick='myRepo("' + item.full_name + '")')
                = item.full_name
        div.six.columns
            = item.open_issues
            |  issues
        div.row
        div.two.columns
            = item.forks_count
            |  forks
        div.three.columns
            = item.updated_at
            |  last updated
        div.one.columns
            = item.watchers
            |  watchers